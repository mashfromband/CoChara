# 開発環境のAPI URL
NEXT_PUBLIC_API_URL=http://localhost:8500

# データベース接続URL
DATABASE_URL="postgresql://cochara:cochara@localhost:5434/cochara?schema=public"

# NextAuth.js設定（公開用途では必ず各自の値を設定）
NEXTAUTH_URL="http://localhost:8500"
# OpenSSLなどで生成した強固なランダム値を設定
NEXTAUTH_SECRET=""

# GitHub OAuth（各自のアプリのクレデンシャルを設定）
GITHUB_ID=""
GITHUB_SECRET=""

# Google OAuth（各自のアプリのクレデンシャルを設定）
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

# Stable Diffusion AUTOMATIC1111 と rembg サーバ設定
# 例) SD_WEBUI_BASE_URL=http://127.0.0.1:7860  (AUTOMATIC1111 を --api で起動)
# 例) REMBG_BASE_URL=http://127.0.0.1:7000   (rembg s --host 0.0.0.0 --port 7000)
SD_WEBUI_BASE_URL=
REMBG_BASE_URL=

# MinIO (S3互換) ストレージ設定（完全セルフホスト）
# 例) MINIO_ENDPOINT=http://127.0.0.1:9000
# 必要に応じてMINIO_FORCE_PATH_STYLE=trueを維持（MinIOはパススタイルが無難）
MINIO_ENDPOINT=
MINIO_ACCESS_KEY=
MINIO_SECRET_KEY=
MINIO_FORCE_PATH_STYLE=true

# Seed（初期管理ユーザー）設定（公開用途では必ず設定推奨）
# 例) SEED_ADMIN_EMAIL=admin@example.com
# 例) SEED_ADMIN_PASSWORD=強固なパスワード
SEED_ADMIN_EMAIL=
SEED_ADMIN_PASSWORD=