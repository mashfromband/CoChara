# 開発環境のAPI URL
NEXT_PUBLIC_API_URL=http://localhost:8500

# データベース接続URL
DATABASE_URL="postgresql://cochara:cochara@localhost:5434/cochara?schema=public"

# NextAuth.js設定（公開用途では必ず各自の値を設定）
NEXTAUTH_URL="http://localhost:8500"
# OpenSSLなどで生成した強固なランダム値を設定
NEXTAUTH_SECRET=""

# Google OAuth（各自のアプリのクレデンシャルを設定）
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

# Stable Diffusion AUTOMATIC1111 と rembg サーバ設定
# 例) SD_WEBUI_BASE_URL=http://127.0.0.1:7860  (AUTOMATIC1111 を --api で起動)
# 例) REMBG_BASE_URL=http://127.0.0.1:7000   (rembg s --host 0.0.0.0 --port 7000)
SD_WEBUI_BASE_URL=
REMBG_BASE_URL=

# Cloudflare R2 (S3互換) 設定 - 開発・本番共通
# R2のS3互換エンドポイント形式: https://<ACCOUNT_ID>.r2.cloudflarestorage.com
# Account ID: 05f6d2c09fa3ee88119588f182550e9c の例
MINIO_ENDPOINT=https://05f6d2c09fa3ee88119588f182550e9c.r2.cloudflarestorage.com
# 本番では十分に強い一意のアクセスキーとシークレットを使用
MINIO_ACCESS_KEY=df9983ee1c9f4fcd65a8fbb70cfb8960
MINIO_SECRET_KEY=2a077cbed921d0c2ee57b2e39149ca2fe0cb7db457a3b7664f17659ba4479039
# MinIO はパススタイルが無難
MINIO_FORCE_PATH_STYLE=false
# 署名付きURLの有効期限（秒）
# 例) 1時間: 3600
S3_SIGNED_URL_EXPIRES=3600

# Next.jsの画像最適化で許可する外部ホスト（署名付きURLのホスト名）
# R2のvhost形式: バケット名.アカウントID.r2.cloudflarestorage.com
# バケット名が「cochara-uploads」の場合の例
IMAGE_REMOTE_HOSTNAMES=cochara-uploads.05f6d2c09fa3ee88119588f182550e9c.r2.cloudflarestorage.com

# Seed（初期管理ユーザー）設定（公開用途では必ず設定推奨）
# 例) SEED_ADMIN_EMAIL=admin@example.com
# 例) SEED_ADMIN_PASSWORD=強固なパスワード
SEED_ADMIN_EMAIL=
SEED_ADMIN_PASSWORD=